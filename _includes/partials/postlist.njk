<div class="postlist">
  {% for post in postslist %}
    <div class="individual_item">
      <div class="item_column">
        <a href="{{ post.url | url }}">
          <img src="{{ post.data.featured_image | url }}" />
        </a>
        <a class="shoe_name" href="{{ post.url | url }}">
          {{ post.data.title }}
        </a>
      </div>
      <div class="item_column">
        <a href="{{ post.url | url }}">
          <img src="{{ post.data.featured_image2 | url }}" />
        </a>
      </div>
    </div>
  {% endfor %}
</div>



